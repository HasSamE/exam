{
  "name": "exam",
  "description": "Test runner",
  "keywords": [
    "exam"
  ],
  "version": "0.0.2",
  "main": "exam.js",
  "bin": "exam.js",
  "homepage": "http://github.com/zerious/exam",
  "repository": "http://github.com/zerious/exam.git",
  "bugs": {
    "url": "http://github.com/zerious/exam/issues"
  },
  "author": {
    "name": "Sam Eubank",
    "email": "sameubank@gmail.com"
  },
  "contributors": [
    {
      "name": "Sam Eubank",
