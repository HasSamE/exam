    }
  ],
  "licenses": [
    {
      "type": "MIT",
      "url": "http://github.com/zerious/exam/blob/master/MIT-LICENSE.md"
    }
  ],
  "engines": [
    "node >= 0.2.6"
  ],
  "scripts": {
    "test": "./exam",
    "retest": "./exam --watch",
    "cover": "istanbul cover ./exam.js --one-core",
    "report": "open coverage/lcov-report/index.html",
    "coveralls": "istanbul cover ./exam.js --report lcovonly -- -R spec && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage"
  },
  "dependencies": {},
  "devDependencies": {
    "coveralls": "2.11.1",
    "istanbul": "0.3.0",
    "zeriousify": "0.1.7",
    "assert-plus": "0.1.5",
    "mocha": "1.18.2"
  }
